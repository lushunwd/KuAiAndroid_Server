<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace必须指向Dao接口 -->
<mapper namespace="com.jd.dao.AdvzbDao">
	<select id="getAdvzbList" resultType="java.util.HashMap" parameterType="map">
	select * from advzhanban order by time desc limit #{0},#{1};
	</select>
</mapper>
