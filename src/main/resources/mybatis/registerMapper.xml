<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jd.dao.RegisterDao">
    <insert id="addUser"   parameterType="map">
        INSERT INTO quser( uname, upass, utime) VALUES ('${uname}','${upassword}','${utime}');
    </insert>

    <select id="getUserInfoByName" resultType="java.util.HashMap"  parameterType="map">
        SELECT * FROM quser WHERE uname='${uname}';
    </select>
</mapper>
